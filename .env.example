################################################
###       environment config file            ###
################################################
SOURCE_DIR=./www


############# Container source.list ############
REPLACE_SOURCE_LIST=true


#################### Nginx #####################
NGINX_VERSION=1.15.7-alpine
NGINX_HTTP_HOST_PORT=80
NGINX_HTTPS_HOST_PORT=443
NGINX_CONFD_DIR=./conf/conf.d
NGINX_CONF_FILE=./conf/nginx.conf
NGINX_LOG_DIR=./logs


#################### PHP7.2 ####################
PHP72_VERSION=7.2.13
PHP72_XDEBUG=2.6.1
PHP72_SWOOLE=4.3.3
####PHP72_SEASLOG=2.0.2
PHP72_REDIS=4.1.1
PHP72_PHP_CONF_FILE=./conf/php72.ini
PHP72_FPM_CONF_FILE=./conf/php72-fpm.conf


#################### PHP5.6 ####################
PHP56_VERSION=5.6.32
PHP56_XDEBUG=2.4.1
PHP56_SWOOLE=false
####PHP56_SEASLOG=false
PHP56_REDIS=false
PHP56_PHP_CONF_FILE=./conf/php56.ini
PHP56_FPM_CONF_FILE=./conf/php56-fpm.conf


#################### MySQL #####################
MYSQL_VERSION=8.0.13
MYSQL_HOST_PORT=3306
MYSQL_ROOT_PASSWORD=123456
MYSQL_DATA_DIR=./mysql
MYSQL_CONF_FILE=./conf/mysql.cnf


#################### Redis #####################
REDIS_VERSION=5.0.3-alpine
REDIS_HOST_PORT=6379
REDIS_CONF_FILE=./conf/redis.conf


################## phpMyAdmin ##################
PHPMYADMIN_HOST_PORT=8080


################# redisMyAdmin #################
REDISMYADMIN_HOST_PORT=8081

